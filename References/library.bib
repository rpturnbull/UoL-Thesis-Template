@article{Harrington2007,
abstract = {The location of the hip joint centre (HJC) is required for calculations of hip moments, the location and orientation of the femur, and muscle lengths and lever arms. In clinical gait analysis, the HJC is normally estimated using regression equations based on normative data obtained from adult populations. There is limited relevant anthropometric data available for children, despite the fact that clinical gait analysis is predominantly used for the assessment of children with cerebral palsy. In this study, pelvic MRI scans were taken of eight adults (ages 23–40), 14 healthy children (ages 5–13) and 10 children with spastic diplegic cerebral palsy (ages 6–13). Relevant anatomical landmarks were located in the scans, and the HJC location in pelvic coordinates was found by fitting a sphere to points identified on the femoral head. The predictions of three common regression equations for HJC location were compared to those found directly from MRI. Maximum absolute errors of 31mm were found in adults, 26mm in children, and 31mm in the cerebral palsy group. Results from regression analysis and leave-one-out cross-validation techniques on the MRI data suggested that the best predictors of HJC location were: pelvic depth for the antero-posterior direction; pelvic width and leg length for the supero-inferior direction; and pelvic depth and pelvic width for the medio-lateral direction. For single-variable regression, the exclusion of leg length and pelvic depth from the latter two regression equations is proposed. Regression equations could be generalised across adults, children and the cerebral palsy group.},
author = {Harrington, M.E. and Zavatsky, A.B. and Lawson, S.E.M. and Yuan, Z. and Theologis, T.N.},
doi = {10.1016/J.JBIOMECH.2006.02.003},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Harrington et al. - 2007 - Prediction of the hip joint centre in adults, children, and patients with cerebral palsy based on magnetic re.pdf:pdf},
issn = {0021-9290},
journal = {Journal of Biomechanics},
month = {jan},
number = {3},
pages = {595--602},
publisher = {Elsevier},
title = {{Prediction of the hip joint centre in adults, children, and patients with cerebral palsy based on magnetic resonance imaging}},
url = {https://www.sciencedirect.com/science/article/pii/S0021929006000583?via{\%}3Dihub},
volume = {40},
year = {2007}
}
