doi = {10.1115/detc2016-60131},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Colombo et al. - 2016 - A Method to Improve Prosthesis Leg Design Based on Pressure Analysis at the Socket-Residual Limb Interface.pdf:pdf},
