journal = {IEEE Systems Journal},
keywords = {Human robot interaction Hydraulic actuators Muscle},
pages = {1068--1081},
title = {{Lower Limb Wearable Robots for Assistance and Rehabilitation: A State of the Art}},
url = {internal-pdf://206.192.240.88/Huo-2016-Lower Limb Wearable Robots for Assist.pdf https://www.scopus.com/inward/record.uri?eid=2-s2.0-84908110271{\&}doi=10.1109{\%}2FJSYST.2014.2351491{\&}partnerID=40{\&}md5=6861eeddc923d507ff60e134cb5ca87f http://ieeexplore.ieee.org/},
volume = {10},
