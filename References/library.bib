issn = {1534-4320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
keywords = {biomedical equipment bone gait analysis human-robo},
pages = {1128--1137},
title = {{Human{\&}{\#}x2013;Robot Interaction: Kinematics and Muscle Activity Inside a Powered Compliant Knee Exoskeleton}},
url = {internal-pdf://213.234.54.156/Knaepen-2014-Human{\&}{\#}x2013{\%}3BRobot Interaction{\_} K.pdf http://ieeexplore.ieee.org/ielx7/7333/6955772/06817616.pdf?tp={\&}arnumber=6817616{\&}isnumber=6955772},
