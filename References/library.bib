doi = {10.1016/j.biotri.2016.04.001},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Cavaco et al. - 2016 - Friction of prosthetic interfaces used by transtibial amputees.pdf:pdf},
issn = {23525738},
journal = {Biotribology},
keywords = {Adhesion force,Coefficient of friction,Elastomer,Surface energy},
pages = {36--41},
publisher = {Elsevier B.V.},
title = {{Friction of prosthetic interfaces used by transtibial amputees}},
url = {http://dx.doi.org/10.1016/j.biotri.2016.04.001},
volume = {6},
year = {2016}
}
