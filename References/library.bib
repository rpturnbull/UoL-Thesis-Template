proportional-EMG-controlled human augmentation devices
exoskeleton},
author = {Haisong, Dong and Matsushita, K and Yamamoto, T and Ishiguro, H},
issn = {1882-4889},
journal = {SICE Journal of Control, Measurement, and System Integration},
keywords = {electromyography human-robot interaction robots},
pages = {312--320},
title = {{On the well-timed assistance in power-assisted sit-to-stand movement}},
volume = {8},
year = {2015}
}
