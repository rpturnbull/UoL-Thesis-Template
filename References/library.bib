publisher = {West Virginia University},
title = {{Design, Fabrication, and Control of an Upper Arm Exoskeleton Assistive Robot}},
url = {http://0-search.proquest.com.wam.leeds.ac.uk/docview/1617417708?accountid=14664 http://openurl.ac.uk/?genre=dissertations+{\%}26+theses{\&}issn={\&}title=Design{\%}2C+Fabrication{\%}2C+and+Control+of+an+Upper+Arm+Exoskeleton+Assistive+Robot{\&}volume={\&}issue={\&}date=2014-01-0},
