issn = {14220067},
journal = {International Journal of Molecular Sciences},
keywords = {Fabrication,Hydrogels,Tissue engineering,Vascular networks},
number = {7},
pages = {15997--16016},
pmid = {26184185},
title = {{Hydrogels for engineering of perfusable vascular networks}},
volume = {16},
