journal = {Journal of Bionic Engineering},
keywords = {human-exoskeleton modelling human response modelli},
pages = {706--715},
title = {{Simulation Architecture for Modelling Interaction Between User and Elbow-articulated Exoskeleton}},
url = {internal-pdf://136.17.139.18/de Kruif-2017-Simulation Architecture for Mode.pdf https://www.sciencedirect.com/science/article/pii/S1672652916604377 https://ac.els-cdn.com/S1672652916604377/1-s2.0-S1672652916604377-main.pdf?{\_}tid=32a4e1ca-1582-11e8-b5e9-000},
volume = {14},
