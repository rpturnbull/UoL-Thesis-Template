isbn = {0169-1864},
issn = {01691864},
journal = {Advanced Robotics},
keywords = {Robot-assisted gait rehabilitation,physical human-robot interaction,pneumatic muscles,powered knee exoskeleton,proxy-based sliding mode control},
number = {5},
pages = {513--535},
title = {{Safe and compliant guidance by a powered knee exoskeleton for robot-assisted rehabilitation of gait}},
type = {Journal Article},
