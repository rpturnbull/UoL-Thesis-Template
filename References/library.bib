@article{Sneve2008,
abstract = {Objective: To explore the relation between serum parathyroid hormone (PTH) and bone mineral density (BMD), adjusted for lifestyle factors including smoking. Design: Cross-sectional study. Methods: The Troms{\o} Study is a population-based study performed for the fifth time in 2001. Serum PTH was measured and the subjects filled in a questionnaire covering lifestyle factors. BMD at the hip, distal and ultradistal forearm was measured. Results: Complete datasets were available in 1442 men and 1368 women. Age, body mass index and serum PTH were strong predictors of BMD level at the hip in both genders. No significant relation was seen between serum PTH and BMD at the distal or ultradistal forearm. When smokers and non-smokers were analysed separately, the relation between PTH and BMD at the hip was significant in current non-smokers only. In males, current non-smokers had significantly higher BMD at all three measurement sites compared with current smokers. Male former smokers had values in between current and never smokers. There was a significant and negative relation between number of years smoked and BMD at the hip. In male former smokers, there was an increase in BMD with increasing years since smoking cessation. Conclusion: Serum PTH is negatively associated with BMD at the hip, and the relation seems to be masked, or diminished, by smoking. Smoking reduces BMD at the hip, distal and ultradistal forearm in males, and the effect appears to be mainly time and not dose dependent. {\textcopyright} 2008 Society of the European Journal of Endocrinology.},
author = {Sneve, Monica and Emaus, Nina and Joakimsen, Ragnar Martin and Jorde, Rolf},
doi = {10.1530/EJE-07-0610},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Sneve et al. - 2008 - The association between serum parathyroid hormone and bone mineral density, and the impact of smoking The Troms{\o} S.pdf:pdf},
issn = {08044643},
journal = {European Journal of Endocrinology},
number = {3},
pages = {401--409},
title = {{The association between serum parathyroid hormone and bone mineral density, and the impact of smoking: The Troms{\o} Study}},
volume = {158},
year = {2008}
}
@article{Dowson2009a,
abstract = {Most tribologists are concerned with engineering applications of their skills, yet little recognition is given to the many illustrations of effective solutions to friction, wear, and lubrication problems evident in our everyday lives. This view will be supported by examples encountered by most of us in a single day, with an open encouragement to all to add to the list. Several of these examples are embedded in biotribology and many were reported before either tribology, in 1966, or biotribology, in 1970 and 1973, was formally defined. It is hoped that this account of a typical ... Tribological day in the life of ... will expose some of the fascinating examples of tribology all around us.},
