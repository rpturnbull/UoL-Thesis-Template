isbn = {1532-2149 (Electronic)$\backslash$r1090-3801 (Linking)},
issn = {10903801},
keywords = {Back Support Exoskeleton,Back pain,Bottom-up approach,Burden of disease,Cost-of-illness,Healthcare utilisation,Lower Back Support Exoskeleton,Lumbar Exoskeleton},
mendeley-tags = {Back Support Exoskeleton,Lower Back Support Exoskeleton,Lumbar Exoskeleton},
