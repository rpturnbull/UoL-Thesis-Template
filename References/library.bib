issn = {1742-4801},
journal = {International Wound Journal},
keywords = {Dressing prophylaxis,Force dispersion,In vitro force measurement,Point load deflection,Poisson's ratio,Pressure ulcer,Pressure ulcer prevention,Shear},
month = {aug},
