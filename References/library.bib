journal = {Procedia Manufacturing},
keywords = {Human-motion-intention Knee exoskeleton Pattern re},
pages = {1379--1386},
title = {{Towards a Robotic Knee Exoskeleton Control Based on Human Motion Intention through EEG and sEMGsignals}},
