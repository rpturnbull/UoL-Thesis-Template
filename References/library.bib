issn = {1672-6529},
journal = {Journal of Bionic Engineering},
keywords = {exoskeleton pHRI measurement data fusion pHRI mini},
pages = {272--283},
title = {{Development and Analysis of an Electrically Actuated Lower Extremity Assistive Exoskeleton}},
