title = {{A Natural Contact Sensor Paradigm for Nonintrusive and Real-Time Sensing of Biosignals in Human-Machine Interactions}},
url = {internal-pdf://240.163.124.65/Lin-2011.pdf https://ieeexplore.ieee.org/ielx5/7361/5697312/05482079.pdf?tp={\&}arnumber=5482079{\&}isnumber=5697312},
