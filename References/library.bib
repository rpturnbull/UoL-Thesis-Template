file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Lin - 2011 - A Natural Contact Sensor Paradigm for Nonintrusive and Real-Time Sensing of Biosignals in Human-Machine Interactions.pdf:pdf},
issn = {1530-437X},
journal = {IEEE Sensors Journal},
keywords = {biosensors body sensor networks cognition man-mach},
