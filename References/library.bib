file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Leal-Junior et al. - 2018 - POF-IMU sensor system A fusion between inertial measurement units and POF sensors for low-cost and highly re.pdf:pdf},
issn = {1068-5200},
journal = {Optical Fiber Technology},
keywords = {Optical fiber sensors Polymer optical fibers Senso},
pages = {82--89},
title = {{POF-IMU sensor system: A fusion between inertial measurement units and POF sensors for low-cost and highly reliable systems}},
url = {internal-pdf://153.151.160.39/1-s2.0-S1068520017306454-main.pdf http://www.sciencedirect.com/science/article/pii/S1068520017306454},
volume = {43},
year = {2018}
}
