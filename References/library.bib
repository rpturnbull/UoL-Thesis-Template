title = {{Development of adjustable knee joint for walking assistance devices}},
url = {internal-pdf://165.59.13.152/Choi-2017-Development of adjustable knee joint.pdf http://ieeexplore.ieee.org/ielx7/8119304/8202121/08205993.pdf?tp={\&}arnumber=8205993{\&}isnumber=8202121},
year = {2017}
