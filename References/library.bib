keywords = {Clinical decision making,Gait analysis,Heel-walking,Step negotiation,Toe-walking},
number = {1},
pages = {6--13},
pmid = {21123071},
publisher = {Elsevier B.V.},
title = {{A multiple-task gait analysis approach: Kinematic, kinetic and EMG reference data for healthy young and adult subjects}},
url = {http://dx.doi.org/10.1016/j.gaitpost.2010.08.009},
volume = {33},
year = {2011}
