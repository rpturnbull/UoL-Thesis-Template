pages = {120201:1--120201:13},
title = {{The relationship between physical human-exoskeleton interaction and dynamic factors: using a learning approach for control applications}},
url = {internal-pdf://0296004431/2014 Huu-Toan et al - relationaship between pH.pdf},
volume = {57},
year = {2014}
}
