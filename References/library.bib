@article{Hemingway2017,
abstract = {Coordinate singularities and gimbal lock are two phenomena that present themselves in models for the dynamics of mechanical systems. The former phenomenon pertains to the coordinates used to parameterize the configuration manifold of the system, while the latter phenomenon has a distinctive physical manifestation. In the present paper, we use tools from differential geometry to show how gimbal lock is intimately associated with an orthogonality condition on the applied forces and moments which act on the system. This condition is equivalent to a generalized applied force being normal to the configuration manifold of the system. Numerous examples, including the classic bead on a rotating hoop example and a gimbaled rigid body, are used to illuminate the orthogonality condition. These examples help to offer a new explanation for the elimination of gimbal lock by the addition of gimbals and demonstrate how integrable constraints alter the configuration manifold and may consequently eliminate coordinate singularities.},
author = {Hemingway, Evan G and O'reilly, Oliver M},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Hemingway, O'reilly - 2018 - Perspectives on Euler angle singularities, gimbal lock, and the orthogonality of applied forces and applied.pdf:pdf},
journal = {Multibody System Dynamics},
keywords = {Configuration,Coordinate,Gimbal,Lock {\textperiodcentered},Manifold,Singularities {\textperiodcentered}},
number = {2018},
pages = {31--56},
title = {{Perspectives on Euler angle singularities, gimbal lock, and the orthogonality of applied forces and applied moments}},
url = {https://www.researchgate.net/publication/313114372},
volume = {44},
year = {2018}
}
@article{Lee2017,
abstract = {This paper proposes a novel control method to minimize muscle energy for power-assistant robotic systems that support the intended motions of a user under unknown external perturbations, using surface electromyogram (sEMG) signals. Conventional control methods based on force/torque (F/T) sensors have limitations to detect human intentions and could, presumably, misunderstand or distort such intentions because of external perturbations of the interaction forces, such as those found in activities of daily living. F/T sensors measure the sum of the applied force, including unknown external forces and human intention; thus, a power-assistant robot controller cannot exactly decompose the real human force. In this paper, we describe a counterexample that cannot be supported by conventional force-sensor-based control methods. We also verify why these control methods may guide human behavior in the wrong direction, and thus, have limitations under unknown external perturbations. We then propose a new control method to minimize the muscle energy indicated by sEMG signals. The proposed control approach is fundamentally based on the concept of power-assistance, in which a robot can reduce the users expended muscle energy while performing given tasks. The proposed control approach is verified through experiments using a power-assistant robotic system for the upper limbs under external perturbations.},
author = {Lee, J and Kim, M and Kim, K},
doi = {10.1109/TNSRE.2017.2723609},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Lee, Kim, Kim - 2017 - A Control Scheme to Minimize Muscle Energy for Power Assistant Robotic Systems Under Unknown External Perturbatio.pdf:pdf},
issn = {1534-4320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
keywords = {body sensor networks electromyography force contro},
pages = {2313--2327},
title = {{A Control Scheme to Minimize Muscle Energy for Power Assistant Robotic Systems Under Unknown External Perturbation}},
url = {internal-pdf://203.111.158.115/Lee-2017-A Control Scheme to Minimize Muscle E.pdf http://ieeexplore.ieee.org/ielx7/7333/8168802/07968501.pdf?tp={\&}arnumber=7968501{\&}isnumber=8168802},
volume = {25},
year = {2017}
}
