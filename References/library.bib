issn = {1672-6529},
journal = {Journal of Bionic Engineering},
keywords = {humanoid arm humanoid hand human robot interaction},
pages = {133--142},
title = {{Development of an Anthropomorphic Robotic Arm and Hand for Interactive Humanoids}},
