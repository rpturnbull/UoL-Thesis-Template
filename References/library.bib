journal = {Scientia Iranica},
keywords = {Large deformation,Large intra-abdominal organs,Mass-spring-damper model,Surgery simulation,Virtual environment},
number = {5},
pages = {1833--1841},
title = {{A mass-spring-damper model for real time simulation of the frictional grasping interactions between surgical tools and large organs}},
volume = {22},
year = {2015}
}
