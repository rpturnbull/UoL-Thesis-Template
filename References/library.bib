file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Wang et al. - 2018 - Robust and high-performance soft inductive tactile sensors based on the Eddy-current effect.pdf:pdf},
issn = {0924-4247},
journal = {Sensors and Actuators A: Physical},
keywords = {Conductive film,Eddy-current effect,Elastomer,Inductive sensor,Planar coil,Tactile sensor},
pages = {44--52},
title = {{Robust and high-performance soft inductive tactile sensors based on the Eddy-current effect}},
url = {https://www.sciencedirect.com/science/article/pii/S0924424717317855},
volume = {271},
year = {2018}
