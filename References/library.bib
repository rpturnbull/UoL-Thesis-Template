@article{Rajagopal2016a,
abstract = {Objective: Musculoskeletal models provide a non-invasive means to study human movement and predict the effects of interventions on gait. Our goal was to create an open-source 3-D musculoskeletal model with high-fidelity representations of the lower limb musculature of healthy young individuals that can be used to generate accurate simulations of gait. Methods: Our model includes bony geometry for the full body, 37 degrees of freedom to define joint kinematics, Hill-type models of 80 muscle-tendon units actuating the lower limbs, and 17 ideal torque actuators driving the upper body. The model's musculotendon parameters are derived from previous anatomical measurements of 21 cadaver specimens and magnetic resonance images of 24 young healthy subjects. We tested the model by evaluating its computational time and accuracy of simulations of healthy walking and running. Results: Generating muscle-driven simulations of normal walking and running took approximately 10 minutes on a typical desktop computer. The differences between our muscle-generated and inverse dynamics joint moments were within 3{\%} (RMSE) of the peak inverse dynamics joint moments in both walking and running, and our simulated muscle activity showed qualitative agreement with salient features from experimental electromyography data. Conclusion: These results suggest that our model is suitable for generating muscle-driven simulations of healthy gait. We encourage other researchers to further validate and apply the model to study other motions of the lower extremity. Significance: The model is implemented in the open-source software platform OpenSim.},
author = {Rajagopal, Apoorva and Dembia, Christopher L. and DeMers, Matthew S. and Delp, Denny D. and Hicks, Jennifer L. and Delp, Scott L.},
doi = {10.1109/TBME.2016.2586891},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Rajagopal et al. - 2016 - Full-Body Musculoskeletal Model for Muscle-Driven Simulation of Human Gait.pdf:pdf},
issn = {15582531},
journal = {IEEE Transactions on Biomedical Engineering},
keywords = {Biomechanics,Walking,gait,musculoskeletal model,running,simulation},
number = {10},
pages = {2068--2079},
pmid = {27392337},
title = {{Full-Body Musculoskeletal Model for Muscle-Driven Simulation of Human Gait}},
volume = {63},
year = {2016}
}
@article{Cho2012,
abstract = {A wearable robot called the ‘Exoskeleton' has been developed to assist in human activity in industrial and medical contexts. The Exoskeleton product interacts with the human body much more closely than do other products. Thus, it is important to know how much force the robot will generate before manufacturing the real Exoskeleton. The easiest and most accurate way to determine the force would be to develop a mock-up model for the physical test, but to do so would be very expensive. Thus, it is preferable to predict the force during the design stage using a virtual prototyping environment. In this research, a combined human-exoskeleton model is generated, and dynamics analysis is performed under two different constraints using the AnyBody Software to predict the effect of the strap connecting the user to the exoskeleton. For this purpose, an AnyBody human model is used after scaling, and a simplified exoskeleton model designed in a CAD system is converted to a dynamic model. Constraints are identified based on the human and exoskeleton models. The dynamic analysis will make it possible to calculate the human joint torque and the interaction force for the human model and the exoskeleton, and the significance of the effect of the strap can be determined.},
author = {Cho, Kilhyun and Kim, Yeonghun and Yi, Dooyoung and Jung, Moonki and Lee, Kunwoo},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Cho et al. - 2012 - Analysis and Evaluation of a Combined Human-Exoskeleton Model Under Two Different Constraints Condition.pdf:pdf},
journal = {International Summit on Human Simulation (ISHS)},
keywords = {exoskeleton,human-centered cad,motion analysis,multi-body dynamics},
pages = {23--25},
title = {{Analysis and Evaluation of a Combined Human-Exoskeleton Model Under Two Different Constraints Condition}},
year = {2012}
}
@article{Tang2018,
abstract = {Increasing skin wetness tends to increase fabric-skin adhesion and friction, resulting in wear discomfort or skin injuries. Here, the magnitude estimation approach was used to assess the stickiness sensation perceived in fabrics. Seven fabric types were wetted by putting onto wet 'skin' surface and dried for different durations to achieve different wetness levels, simulating wearing conditions during the recovery period after sweating. Results showed that the relationship between magnitude estimates of stickiness and amount of water present in fabric demonstrated a power function. The exponents and constant from power regression show the growth rate of stickiness sensation with moisture intensity and the perceived stickiness under fixed stimulus intensity, respectively. A novel parameter, accumulated stickiness magnitude (ASM), describing how much discomfort a wetted fabric offered throughout the drying period, was developed. Thin cotton fabrics (fabric W01 and W03), having higher saturation level after contacting with wetted skin surface, arouse stronger stickiness feeling and their ASM is remarkably higher. The difference in stickiness estimates is due to the difference in chemical composition and surface geometry. This study suggests us the way to predict perceived stickiness in fabrics with different wetness levels which is useful for applications like sportswear, intimate apparel or healthcare products.},
author = {Tang, Ka Po Maggie and Chau, Kam Hong and Kan, Chi Wai and Fan, Jin Tu},
doi = {10.1098/rsos.180860},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/Tang et al. - 2018 - Assessing the accumulated stickiness magnitude from fabric-skin friction Effect of wetness level of various fabrics.pdf:pdf},
isbn = {0000000231923},
issn = {20545703},
journal = {Royal Society Open Science},
keywords = {Adhesion,Fabric,Friction,Magnitude estimate,Stickiness sensation,Wet skin},
number = {8},
title = {{Assessing the accumulated stickiness magnitude from fabric-skin friction: Effect of wetness level of various fabrics}},
volume = {5},
year = {2018}
}
@article{DeSantis2008,
abstract = {A broad spectrum of issues have to be addressed in order to tackle the problem of a safe and dependable physical Human–Robot Interaction (pHRI). In the immediate future, metrics related to safety and dependability have to be found in order to successfully introduce robots in everyday enviornments. While there are certainly also “cognitive” issues involved, due to the human perception of the robot (and vice versa), and other objective metrics related to fault detection and isolation, our discussion focuses on the peculiar aspects of “physical” interaction with robots. In particular, safety and dependability are the underlying evaluation criteria for mechanical design, actuation, and control architectures. Mechanical and control issues are discussed with emphasis on techniques that provide safety in an intrinsic way or by means of control components. Attention is devoted to dependability, mainly related to sensors, control architectures, and fault handling and tolerance. Suggestions are provided to draft metrics for evaluating safety and dependability in pHRI, and references to the works of the scientific groups involved in the pHRI research complete the study. The present atlas is a result of the EURON perspective research project “Physical Human–Robot Interaction in anthropic DOMains (PHRIDOM)”, aimed at charting the new territory of pHRI, and constitutes the scientific basis for the ongoing STReP project “Physical Human–Robot Interaction: depENDability and Safety (PHRIENDS)”, aimed at developing key components for the next generation of robots, designed to share their environment with people.},
author = {{De Santis}, Agostino and Siciliano, Bruno and {De Luca}, Alessandro and Bicchi, Antonio},
doi = {https://doi.org/10.1016/j.mechmachtheory.2007.03.003},
file = {:D$\backslash$:/OneDrive - University of Leeds/Literature/De Santis et al. - 2008 - An atlas of physical human–robot interaction.pdf:pdf},
issn = {0094-114X},
journal = {Mechanism and Machine Theory},
keywords = {Physical human–robot interaction in anthropic doma},
pages = {253--270},
title = {{An atlas of physical human–robot interaction}},
url = {internal-pdf://1.112.116.175/De Santis-2008.pdf https://www.sciencedirect.com/science/article/pii/S0094114X07000547 https://ac.els-cdn.com/S0094114X07000547/1-s2.0-S0094114X07000547-main.pdf?{\_}tid=fca35cd9-14d1-4a3a-8e76-68dd2857a484{\&}acdnat=1537345826{\_}9d04},
volume = {43},
year = {2008}
