journal = {Robotics and Autonomous Systems},
keywords = {Prosthesis Knee Ankle Energy efficiency Transfemor},
pages = {4--15},
title = {{Ankle–knee prosthesis with active ankle and energy transfer: Development of the CYBERLEGs Alpha-Prosthesis}},
